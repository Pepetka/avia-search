{
  "name": "avia-search",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "ts-node --esm server/index.ts",
    "server:dev": "nodemon --watch server --ext ts --exec npm run server",
